generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model products {
  id_products Int @id @default(autoincrement())
  name_products String @db.VarChar(255)
  brand String @db.VarChar(255)
  stock Int
}
